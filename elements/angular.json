{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "elements",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush"
    }
  },
  "projects": {
    "elements": {
      "projectType": "application",
      "root": "apps/elements",
      "sourceRoot": "apps/elements/src",
      "prefix": "elements",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "../assets/elements",
            "index": "apps/elements/src/index.html",
            "main": "apps/elements/src/main.ts",
            "polyfills": "apps/elements/src/polyfills.ts",
            "tsConfig": "apps/elements/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/elements/src/favicon.ico", "apps/elements/src/assets"],
            "styles": ["apps/elements/src/styles.scss"],
            "scripts": [],
            "singleBundle": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/elements/src/environments/environment.ts",
                  "with": "apps/elements/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "elements:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "elements:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "elements:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/elements/src/**/*.ts", "apps/elements/src/**/*.html"]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/elements"],
          "options": {
            "jestConfig": "apps/elements/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "elements-e2e": {
      "root": "apps/elements-e2e",
      "sourceRoot": "apps/elements-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/elements-e2e/cypress.json",
            "tsConfig": "apps/elements-e2e/tsconfig.e2e.json",
            "devServerTarget": "elements:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "elements:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/elements-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
