    "@astrojs/sitemap": "3.6.0",
    "@date-fns/tz": "^1.2.0",
    "@fontsource/source-code-pro": "5.2.7",
    "@fontsource/zen-kaku-gothic-new": "5.2.7",
    "@google-cloud/storage": "7.17.2",
    "@resvg/resvg-js": "2.6.2",
    "astro": "5.16.5",
    "cheerio": "1.1.2",
    "date-fns": "4.1.0",
    "escape-html": "^1.0.3",
    "katex": "0.16.27",
    "mermaid": "11.12.1",
