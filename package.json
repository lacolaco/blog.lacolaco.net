    "@iconify/json": "^2.2.161",
    "@iconify/tailwind": "^1.0.0",
    "@lacolaco/notion-sync": "^3.0.0",
    "@notionhq/client": "5.3.0",
    "@tailwindcss/vite": "^4.1.4",
