vedInfo = coreInject(Router).getCurrentNavigation()?.extras?.info;\n            return true;\n          }]\n        },\n      ]);\n\n      await router.navigateByUrl('/simple', {info: 'navigation info'});\n      expect(observedInfo).toEqual('navigation info');\n    });","href":null}],"language":"typescript"}},{"object":"block","id":"62ba9bd8-296b-4374-b9e5-0a4f40b2d550","parent":{"type":"page_id","page_id":"d0a077fb-608b-42d7-ad52-32bbb9c37d0d"},"created_time":"2023-12-07T11:21:00.000Z","last_edited_time":"2023-12-07T11:40:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"これでこの変更のだいたいの雰囲気はわかるだろう。","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"これでこの変更のだいたいの雰囲気はわかるだろう。","href":null}],"color":"default"}},{"object":"block","id":"b8dc0eea-e597-4d54-be1e-b060fc304a30","parent":{"type":"page_id","page_id":"d0a077fb-608b-42d7-ad52-32bbb9c37d0d"},"created_time":"2023-12-07T11:21:00.000Z","last_edited_time":"2023-12-07T11:29:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"type":"heading_2","heading_2":{"rich_text":[{"type":"text","text":{"content":"モチベーション","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"モチベーション","href":null}],"is_toggleable":false,"color":"default"}},{"object":"block","id":"ef17a90c-3746-4b44-8f95-0eaa21063762","parent":{"type":"page_id","page_id":"d0a077fb-608b-42d7-ad52-32bbb9c37d0d"},"created_time":"2023-12-07T11:29:00.000Z","last_edited_time":"2023-12-07T11:30:00.000Z","created_by":{"object"