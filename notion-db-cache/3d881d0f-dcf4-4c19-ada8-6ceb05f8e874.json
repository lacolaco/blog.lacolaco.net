ge_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:30:00.000Z","last_edited_time":"2023-09-08T10:30:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"https://angular.jp/api/core/APP_INITIALIZER#usage-notes","link":{"url":"https://angular.jp/api/core/APP_INITIALIZER#usage-notes"}},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"https://angular.jp/api/core/APP_INITIALIZER#usage-notes","href":"https://angular.jp/api/core/APP_INITIALIZER#usage-notes"}],"color":"default"}},{"object":"block","id":"b6fe9b20-f6d9-435e-a266-adf5c103b717","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:30:00.000Z","last_edited_time":"2023-09-08T10:52:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"たとえば、アプリケーションのログインユーザー情報を外部のサービス（たとえばGoogle AnalyticsやSentryなど）に送信したいケース。一般化すれば、アプリケーション内部の状態の変化を一方的に外部サービスへ同期したいような場面では、状態の変更を購読する形で実装したい。状態がSignalであれば、次のように副作用として記述できる。","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"たとえば、アプリケーションのログインユーザー情報を外部のサービス（たとえばGoogle AnalyticsやSentryなど）に送信したいケース。一般化すれば、アプリケーション内部の状態の変化を一方的に外部サービスへ同期したいような場面では、状態の変更を購読する形で実装したい。状態がSignalであれば、次のように副作用として記述できる。","href":null}],"color":"default"}},{"object":"block","id":"2e583f79-9e85-46e3-80d6-c17bab08110a","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:33:00.000Z","last_edited_time":"2023-09-08T10:44:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"code","code":{"caption":[],"rich_text":[{"type":"text","text":{"content":"// Sentry.setUserのようなもののイメージ\nconst analyticsService = {\n  setUser: (user: User | null) => {\n    if (user) {\n      console.log(`set user: ${user.name}`);\n    } else {\n      console.log(`unset user`);\n    }\n  },\n};\n\n// ユーザー情報が更新されるたびにsetUserする副作用\nfunction bindUserToAnalytics($user: Signal<User | null>) {\n  effect(() => {\n    const user = $user();\n    analyticsService.setUser(user);\n  });\n}","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"// Sentry.setUserのようなもののイメージ\nconst analyticsService = {\n  setUser: (user: User | null) => {\n    if (user) {\n      console.log(`set user: ${user.name}`);\n    } else {\n      console.log(`unset user`);\n    }\n  },\n};\n\n// ユーザー情報が更新されるたびにsetUserする副作用\nfunction bindUserToAnalytics($user: Signal<User | null>) {\n  effect(() => {\n    const user = $user();\n    analyticsService.setUser(user);\n  });\n}","href":null}],"language":"typescript"}},{"object":"block","id":"a6b5e1b1-6580-4931-8cb5-f8a3467a918a","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:31:00.000Z","last_edited_time":"2023-09-08T10:43:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"ではこの ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"ではこの ","href":null},{"type":"text","text":{"content":"effect","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"effect","href":null},{"type":"text","text":{"content":" をどう呼び出すかというと、 ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" をどう呼び出すかというと、 ","href":null},{"type":"text","text":{"content":"APP_INITIALIZER","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"APP_INITIALIZER","href":null},{"type":"text","text":{"content":" のファクトリー関数で呼び出す。 ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" のファクトリー関数で呼び出す。 ","href":null},{"type":"text","text":{"content":"useFactory","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"useFactory","href":null},{"type":"text","text":{"content":" 関数のスコープはインジェクションコンテキストである。 ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" 関数のスコープはインジェクションコンテキストである。 ","href":null},{"type":"text","text":{"content":"inject()","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"inject()","href":null},{"type":"text","text":{"content":" が呼び出せるということは ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" が呼び出せるということは ","href":null},{"type":"text","text":{"content":"effect()","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"effect()","href":null},{"type":"text","text":{"content":" も呼び出せる。このようにアプリケーションの初期化のタイミングで副作用を宣言しておけば、アプリケーション側からはまったく関心を向けずにおくことができる。","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" も呼び出せる。このようにアプリケーションの初期化のタイミングで副作用を宣言しておけば、アプリケーション側からはまったく関心を向けずにおくことができる。","href":null}],"color":"default"}},{"object":"block","id":"ce73518d-c141-441a-b75a-96ca7860fa6d","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:35:00.000Z","last_edited_time":"2023-09-08T10:35:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"code","code":{"caption":[],"rich_text":[{"type":"text","text":{"content":"bootstrapApplication(App, {\n  providers: [\n    {\n      provide: APP_INITIALIZER,\n      multi: true,\n      useFactory: () => {\n        const userAuth = inject(UserAuthService);\n        bindUserToAnalytics(userAuth.$currentUser);\n\n        return () => {};\n      },\n    },\n  ],\n});","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"bootstrapApplication(App, {\n  providers: [\n    {\n      provide: APP_INITIALIZER,\n      multi: true,\n      useFactory: () => {\n        const userAuth = inject(UserAuthService);\n        bindUserToAnalytics(userAuth.$currentUser);\n\n        return () => {};\n      },\n    },\n  ],\n});","href":null}],"language":"typescript"}},{"object":"block","id":"dc1ec0f5-06c0-4323-84f6-bff7079029d9","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:31:00.000Z","last_edited_time":"2023-09-08T10:41:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"いままではこういうアプリケーション初期化時の宣言的コードは ","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"いままではこういうアプリケーション初期化時の宣言的コードは ","href":null},{"type":"text","text":{"content":"AppComponent","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":true,"color":"default"},"plain_text":"AppComponent","href":null},{"type":"text","text":{"content":" に担わせがちだったが、この形のほうがうまく関心を分離できている。","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":" に担わせがちだったが、この形のほうがうまく関心を分離できている。","href":null}],"color":"default"}},{"object":"block","id":"f9c3d96e-56ac-4b0b-be59-95be3dc5751b","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:31:00.000Z","last_edited_time":"2023-09-08T10:57:00.000Z","created_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"last_edited_by":{"object":"user","id":"f4f222d4-d508-405d-ba6c-da82ee26ee54"},"has_children":false,"archived":false,"in_trash":false,"type":"paragraph","paragraph":{"rich_text":[{"type":"text","text":{"content":"何もしない関数を返しているのが気持ち悪ければ、初期化関数をインジェクションコンテキストにしてしまってもよいが、結果にほとんど違いはない。汎用的にユーティリティ関数を作れば、次のようにまとめられる。","link":null},"annotations":{"bold":false,"italic":false,"strikethrough":false,"underline":false,"code":false,"color":"default"},"plain_text":"何もしない関数を返しているのが気持ち悪ければ、初期化関数をインジェクションコンテキストにしてしまってもよいが、結果にほとんど違いはない。汎用的にユーティリティ関数を作れば、次のようにまとめられる。","href":null}],"color":"default"}},{"object":"block","id":"0c767ad1-3363-42bc-acd1-b600c2607ea8","parent":{"type":"page_id","page_id":"3d881d0f-dcf4-4c19-ada8-6ceb05f8e874"},"created_time":"2023-09-08T10:30:00.000Z","last_ed